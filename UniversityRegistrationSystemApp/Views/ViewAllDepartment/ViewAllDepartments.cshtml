@using UniversityRegistrationSystemApp.Model
@model IEnumerable<UniversityRegistrationSystemApp.Model.Department>
@{
    ViewBag.Title = "ViewAllDepartments";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Departments</title>
    <link href="~/Scripts/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Scripts/StyleSheet1.css" rel="stylesheet" />
</head>

<body>

    <div class="center"><h3>View All Department</h3></div>

<table class="center1">
    <thead>
    <tr>
        <td>Code</td>
        <td>Name</td>
    </tr>
    </thead>
    <tbody>
    @foreach (Department department in Model)
    {
        <tr>
            <td>@department.Code</td>
            <td>@department.Name</td>
        </tr>
    }

    </tbody>
</table>
</body>

</html>
